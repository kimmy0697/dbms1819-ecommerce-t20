<style>



</style>
<body class="background">
<div class="container-fluid">
	<div class="row">
		<div class="col-md-3">
	    <div class="sidenav img-icons">
	      <a href="/admin"><h5>DELISHOES ADMIN</h5></a>
	      <hr>
	      <a href="/developers"><img src="/about.png">&ensp;About Developers</a>
	      <a href="/admin-products"><img src="/products.png">&ensp;Products</a>
	      <a href="/categories"><img src="/category.png">&ensp;Categories</a>
	      <a href="/brands"><img src="/brand.png">&ensp;Brands</a>
	      <a href="/orders"><img src="/order.png">&ensp;Orders</a>
	      <a href="/customers"><img src="/customer.png">&ensp;Customers</a>
	    </div>
  	</div>
		<div class="col-md-9">
			<div class="table-responsive">
				<table class="table table-striped table-hover table-design">
					<thead>
						<tr>
							<th>First Name</th>
							<th>Middle Name</th>
							<th>Last Name</th>
							<th>Email Address</th>
							<th>Customer Details</th>
						</tr>
					</thead>
					{{#each rows}}
					<tbody>
						<tr>
							<td>{{this.first_name}}</td>
							<td>{{this.middle_name}}</td>
							<td>{{this.last_name}}</td>
							<td>{{this.email}}</td>
							<td>
								<a href="/admin/customers/{{this.id}}">More details&nbsp;&raquo;</a>
							</td>
						</tr>
					</tbody>
					{{/each}}
				</table>
			</div>
		</div>
	</div>
</div>
</body>