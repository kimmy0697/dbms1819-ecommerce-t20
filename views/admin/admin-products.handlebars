<body class="background">
<div class="container-fluid">
  <div class="row">
    <div class="col-md-3">
      <div class="sidenav img-icons">
        <a href="/admin"><h5>DELISHOES ADMIN</h5></a>
        <hr>
        <a href="/admin/developers"><img src="/about.png">&ensp;About Developers</a>
        <a href="/products"><img src="/products.png">&ensp;Products</a>
        <a href="/admin/categories"><img src="/category.png">&ensp;Categories</a>
        <a href="/admin/brands"><img src="/brand.png">&ensp;Brands</a>
        <a href="/admin/orders"><img src="/order.png">&ensp;Orders</a>
        <a href="/admin/customers"><img src="/customer.png">&ensp;Customers</a>
      </div>
    </div>
    <div class="col-md-9">
      <div class="table-responsive">
        <table class="table table-striped table-hover table-design">
          <thead>
            <tr>
              <th>Product Name</th>
              <th>Brand</th>
              <th>Category</th>
              <th>Price</th>
              <th>Description</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td><img src="{{img}}" style="float:left; height:50px; width:50px;">&ensp;{{name}}</td>
              <td>{{brandname}}</td>
              <td>{{category}}</td>
              <td>{{price}}</td>
              <td>{{description}}</td>
              <td><img src="/action.png" style="height:25px; width:25px;"></td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
</body>